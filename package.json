{
  "name": "express-authentication-starter-api",
  "version": "0.0.1",
  "description": "A small, easily extendable base API for authentication, written with Express.js",
  "main": "server.js",
  "scripts": {
    "mongo:start": "docker run --name realworld-mongo -p 27017:27017 mongo & sleep 5",
    "mongo:stop": "docker stop realworld-mongo && docker rm realworld-mongo",
    "start": "set HTTPS=true && node server.js",
    "dev": "nodemon server.js",
    "test": "mocha --timeout 10000",
    "stop": "lsof -ti :3000 | xargs kill"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BCooperA/express-authentication-starter-api.git"
  },
  "author": "Tatu Kulmala",
  "license": "GPL",
  "bugs": {
    "url": ""
  },
  "homepage": "./",
  "dependencies": {
    "async": "^2.6.2",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "2.7.1",
    "dotenv": "^5.0.0",
    "email-templates": "^2.7.1",
    "errorhandler": "1.4.3",
    "express": "^4.17.0",
    "express-boom": "^2.0.0",
    "express-jwt": "^5.3.0",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "helmet": "^3.18.0",
    "hoek": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^2.3.10",
    "methods": "1.1.2",
    "moment": "^2.24.0",
    "mongoose": "^4.13.18",
    "mongoose-unique-validator": "1.0.2",
    "morgan": "^1.9.1",
    "nodemailer": "^4.7.0",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "1.0.0",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.3",
    "q": "^1.5.1",
    "rand-token": "^0.4.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "util": "^0.11.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "mocha": "^2.4.5"
  }
}
